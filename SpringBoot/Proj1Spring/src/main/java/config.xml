<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

<bean class="com.example.spring.Proj1Spring.Student" id="st">
<property name="roll" value="13"></property>
<property name="name" value="sumit"></property>
<property name="age" value="22"></property>
</bean>

<bean class="com.example.spring.Proj1Spring.JavaTeacher" id="jt">
<property name="teacher_id" value="14"></property>
<property name="teacher_name" value="Amit"></property>
</bean>

<bean class="com.example.spring.Proj1Spring.PythonTeacher" id="pt">
<property name="teacher_id" value="15"></property>
<property name="teacher_name" value="Ankit"></property>
</bean>

<bean class="com.example.spring.Proj1Spring.SqlTeacher" id="st1">
<property name="teacher_id" value="16"></property>
<property name="teacher_name" value="Ajay"></property>
</bean>

</beans>
