<beans xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

<bean class="com.example.spring.Proj4Spring.Employee" id="emp">

<property name="technologies">
<list>
<value>Java</value>
<value>Python</value>
<value>SQL</value>
</list>
</property>

<property name="projects">
<set>
<value>Project-1</value>
<value>Project-2</value>
<value>Project-3</value>
</set>
</property>

<property name="Companies">
<map>
<entry key="1" value="nav"></entry>
<entry key="2" value="soft"></entry>
<entry key="3" value="weird"></entry>
</map>
</property>

</bean>

</beans>
